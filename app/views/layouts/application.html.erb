<!DOCTYPE html>
<html>
  <head>
    <title>PLAY GAMES</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <script src="https://cdn.jsdelivr.net/npm/bs-custom-file-input/dist/bs-custom-file-input.js?date=200618"></script>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/lightbox2/2.7.1/css/lightbox.css?date=200618" rel="stylesheet">
    <%# <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js?date=200618"></script> %>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/lightbox2/2.7.1/js/lightbox.min.js?date=200618" type="text/javascript"></script>
  </head>

  <body>
    <%= render 'layouts/flashes' %>
    <%= render 'common/header' %>
    <%= yield %>
    <%= render 'common/footer'%>
  </body>
</html>
